{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["airbnb-base", "airbnb-typescript/base", "prettier"],
	"plugins": ["prettier", "import"],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"prettier/prettier": "error",
		"no-console": "off",
		"import/prefer-default-export": "off"
	}
}
